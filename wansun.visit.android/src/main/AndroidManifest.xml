            android:value="H8hwKCV0LzHK6wXuOp28D05q4oozGAfG"/>

        <activity
            android:name=".ui.activity.LoginActiovity"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

